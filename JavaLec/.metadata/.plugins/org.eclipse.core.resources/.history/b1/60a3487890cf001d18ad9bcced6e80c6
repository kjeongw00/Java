package chap19_multithread;

public class _01_useOfThread {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Thread thread = new Thread(new Runnable() {
			@Override
			public void run() {
				for(int i = 1; i <= 5; i ++) {
					System.out.println(i);
				}
			}
		});
	}

}
