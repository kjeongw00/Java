package chap03_operator;

public class _03_logicalOperator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//1. 관계 연산자
		int num1 = 100;
		int num2 = 300;
		
		System.out.println("num1 < num2 : " 
							+ (num1 < num2));
		
		System.out.println("num1 >= num2 : " 
							+ (num1 >= num2));
		
		System.out.println("num1 == num2 : " 
							+ (num1 == num2));
		System.out.println("-------------------");
		
		//2. 논리 연산자
		int num3 = 10;
		int num4 = 20;
		
		System.out.println("num1 < num2 &&" +
				  "num3 / num4 == 0" 
				+ (num1 < num2 && num3 / num4 == 0));
		
		System.out.println("false || num4"
				+ " * num2 < 100" 
				+ (false || num4 * num2 < 100));
		
		
		
		
		
		
		
		
		
	}

}
