package chap02_variables;

public class _13_typeCasting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//1. 묵시적(자동) 형변환
		//바이트크기가 작은 타입에서 
		//큰 타입으로는 자동 형변환
		short sNum = 10;
		int iNum = sNum;
		
		//세밀한 표현범위를 갖는 타입으로 자동 형변환
		long lNum = 100L;
		
		
		
		
		
		
		
		
	}

}
